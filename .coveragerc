[run]
branch = True
include = 
    *
    tests/*

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise NotImplementedError
    raise NotImplementedError()
    if __name__ == .__main__.:
    except ImportError:
ignore_errors = True
include = 
    *
    tests/*
omit =
    /usr/*
    *opt*
    *eggs*
